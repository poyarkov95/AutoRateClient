<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.userauthorisation">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login.LoginPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".createaccount.CreateAccountActivity" />
        <activity android:name=".afterloginlogic.AfterLoginActivity" />
        <activity android:name=".recyclerview_all_services.RecyclerViewActivity" />
        <activity android:name=".contuctus.ContactUsActivity" />
        <activity android:name=".autoservice_detail_activity.AutoServiceDetailActivity" />
        <activity android:name=".recycler_view_services_info.RecyclerViewServicesInfoActivity"></activity>
    </application>

</manifest>